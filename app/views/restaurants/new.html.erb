<div class="my-container">
  <h1 class="mb-3">New Restaurant</h1>
  <%= simple_form_for(@restaurant) do |f| %>
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :category %>
    <%= f.input :phone_number %>
    <%= f.submit class: "btn btn-primary" %>
    <%= link_to "Back", restaurants_path, class: "btn btn-primary" %>
  <% end %>
</div>
